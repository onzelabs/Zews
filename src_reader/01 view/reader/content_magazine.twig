{% if page==1 %}
  <div class="page-header">
    <h2>{{ header }}</h2>
  </div>
{% endif %}
{% for item in items %}
<div class="media">
    <div class="media-left">
        <a href="#">
            <img class="media-object" data-src="holder.js/128x128" alt="128x128" src="data:image/svg+xml;base64,"
                data-holder-rendered="true" style="width: 128px; height: 128px;">
        </a>
    </div>
    <div class="media-body">
        <h3 class="media-heading"><a href="{{ item.link }}">{{ item.title }}</a></h3>
        <p><span class="label label-default">{{ item.tracker }}</span> · {{ item.author }} · <small>{{item.pubdate }}</small></p>
        {{ item.description }}
    </div>
</div>
{% endfor %}
